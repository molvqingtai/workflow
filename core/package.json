{
  "name": "@whatfa/workflow",
  "version": "1.0.0",
  "description": "Type-safe workflow primitives for long-running TypeScript agents.",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown src/index.ts --dts --format esm --sourcemap --watch",
    "build": "tsdown src/index.ts --dts --format esm --sourcemap --minify --clean",
    "lint": "eslint --fix --cache",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "workflow",
    "task"
  ],
  "author": "molvqingtai",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
