{
  "branches": [
    "master"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "cp -f ../README.md README.md",
        "successCmd": "(git ls-files README.md --error-unmatch >/dev/null 2>&1 && git checkout -- README.md) || rm -f README.md",
        "failCmd": "(git ls-files README.md --error-unmatch >/dev/null 2>&1 && git checkout -- README.md) || rm -f README.md"
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/github",
    "@semantic-release/git"
  ]
}
